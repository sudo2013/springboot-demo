<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 相对路径与Bat4WindowsService.exe同目录，可配置绝对路径 -->
    
    <!-- 服务状态日志文件 -->
    <add key="LogFilePath" value="Bat4WindowsService.log"/>
    <!-- 局部环境变量 -->
    <add key="SetenvBatFilePath" value="setenv.bat"/>
    <!-- 启动脚本 -->
    <add key="StartupBatFilePath" value="startup.bat"/>
    <!-- 停止脚本-->
    <add key="ShutdownBatFilePath" value="shutdown.bat"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>